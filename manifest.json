{

  "manifest_version": 2,
  "name": "GitHub Simplify Test",
  "version": "1.0",

  "applications": {
    "gecko": {
      "id": "borderify@mozilla.org"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["gitdone.js"]
    }
  ],
  "web_accessible_resources": [
    "toolbar/new_task.png",
    "toolbar/new_project.png",
    "toolbar/projects",
    "toolbar/notiications"
  ]

}
